pragma solidity >0.5.0<0.9.0;

contract Property{
    int public price=100;
    string constant public location="Singapore";

    // price =100l this is not permitted in solidity
    
    function f1() public pure returns(int){
    int x=5;
    x=x*2;
    return x;
    }    
}
    //string is saved in storage by default, so we use "memory"
    
// EVM saves data in 3 places: 1)Storage 2)Stack 3) Memory
