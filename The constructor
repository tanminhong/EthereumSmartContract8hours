pragma solidity>=0.5.0<0.9.0;
contract Property{
     int public price;
     string public location;
     address public owner;
     
     constructor(int _price, string memory _location){
         price= _price;
         location = _location;
         owner =msg.sender;
     }
     
    function setPrice(int _price) public{
        price = _price;
        
        //msg.sender this initializes the value of msg.sender where msg.sender is a global built in variable. 
        function setLocation(string memory _location) public{
        location= _location;
    
    }
}
